{
  "maxerr": 50,
  // Enforcing
  "bitwise": true,
  "camelcase": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 4,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "undef": true,
  "unused": true,
  "maxparams": false,
  "maxdepth": 4,
  "maxstatements": 15,
  "maxcomplexity": false,

  // Environments
  "devel": true, // Development/debugging (alert, confirm, etc)
  "node": true
}
